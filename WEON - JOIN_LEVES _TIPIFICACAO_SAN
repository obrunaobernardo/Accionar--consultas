SELECT
 
 NOME_AGENTE
,NOME_CONTATO
,VOLUME_MENSAGENS
,QUALIFICACAO_FINAL
,OCORRENCIA 
,CPC
,SUCESSO

FROM [ipo].[dbo].[WEON_LEVES_TOTAL] AS WLT
INNER JOIN [santander].[dbo].[SAN_TIPIFICACAO] AS SANTIP ON WLT.QUALIFICACAO_FINAL = SANTIP.OCORRENCIA
WHERE CPC = 1 AND SUCESSO = 1 --AND QUALIFICACAO_FINAL = 'VENDA VALORADA'


--ORDER BY SUCESSOa


