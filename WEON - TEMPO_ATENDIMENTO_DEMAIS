SELECT 
 
 --MAX(TEMPO_ATENDIMENTO) AS MAX_TEMPO
--,MIN(TEMPO_ATENDIMENTO) AS MIN_TEMPO
 --NOME_AGENTE
 TEMPO_ATENDIMENTO

FROM [ipo].[dbo].[WEON_DEMAIS] AS WD
INNER JOIN [santander].[dbo].[SAN_TIPIFICACAO] AS SANTIP ON WD.QUALIFICACAO_FINAL = SANTIP.OCORRENCIA
WHERE CPC = 1 AND SUCESSO = 1 AND QUALIFICACAO_FINAL = 'ACORDO' --AND TEMPO_ATENDIMENTO < '00:00:30'
