USE santander

SELECT 
 
distinct TABULACAO
,COUNT(TABULACAO) AS TOTAL_POR_TABULACAO

FROM NOVO_SAN_DISCAGENS AS SD
INNER JOIN [santander].[dbo].[SAN_TIPIFICACAO] AS SANTIP ON SD.TABULACAO COLLATE Latin1_General_CS_AI  = SANTIP.OCORRENCIA COLLATE Latin1_General_CS_AI
WHERE (DATA = '2024-07-03' OR DATA = '2024-07-05' OR DATA = '2024-07-11' OR DATA = '2024-07-25' OR DATA = '2024-07-29') 
AND NOME_OPERADOR IS NOT NULL AND TIPO = 'SUCESSO' AND SD.CPC = 1 AND SUCESSO = 1
GROUP BY TABULACAO
ORDER BY TOTAL_POR_TABULACAO
