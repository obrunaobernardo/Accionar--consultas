SELECT
 
 --MAX(TEMPO_ATENDIMENTO) AS MAX_TEMPO
--,MIN(TEMPO_ATENDIMENTO) AS MIN_TEMPO
--NOME_AGENTE
TEMPO_ATENDIMENTO
FROM [ipo].[dbo].[WEON_LEVES_TOTAL] AS WLT
INNER JOIN [santander].[dbo].[SAN_TIPIFICACAO] AS SANTIP ON WLT.QUALIFICACAO_FINAL = SANTIP.OCORRENCIA
WHERE CPC = 1 AND SUCESSO = 1 AND QUALIFICACAO_FINAL = 'ACORDO' AND TEMPO_ATENDIMENTO < '00:00:30'

