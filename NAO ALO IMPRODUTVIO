USE santander

SELECT 
 SANTIP.OCORRENCIA
,ISNULL(COUNT(SANDIS.CPC), 0) AS QUANTIDADE_CPC
,TIPO
,SANDIS.DATA
FROM [dbo].[SAN_TIPIFICACAO] AS SANTIP 
   
LEFT JOIN NOVO_SAN_DISCAGENS AS SANDIS ON SANTIP.OCORRENCIA COLLATE Latin1_General_CS_AI = SANDIS.TABULACAO COLLATE Latin1_General_CS_AI 
AND SANDIS.ALO = 0 
AND SANDIS.PERIODO = 202408 
AND SANDIS.DATA = '2024-08-28' 
WHERE SANTIP.TIPO = 'IMPRODUTIVO' AND (SANTIP.OCORRENCIA = 'CONTRATO NAO LOCALIZADO' OR SANTIP.OCORRENCIA = 'SEM INTERACAO NO DIGITAL') 
GROUP BY SANTIP.OCORRENCIA, SANTIP.TIPO, SANDIS.DATA, MACRO
ORDER BY MACRO, SANTIP.OCORRENCIA;
