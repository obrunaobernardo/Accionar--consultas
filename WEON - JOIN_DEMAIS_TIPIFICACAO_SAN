SELECT 
 
 NOME_AGENTE
,NOME_CONTATO
,VOLUME_MENSAGENS
,QUALIFICACAO_FINAL
,OCORRENCIA 
,CPC
,SUCESSO

FROM [ipo].[dbo].[WEON_DEMAIS] AS WD
INNER JOIN [santander].[dbo].[SAN_TIPIFICACAO] AS SANTIP ON WD.QUALIFICACAO_FINAL = SANTIP.OCORRENCIA
WHERE CPC = 1 AND SUCESSO = 1 AND QUALIFICACAO_FINAL = 'ACORDO'



--ORDER BY SUCESSO

