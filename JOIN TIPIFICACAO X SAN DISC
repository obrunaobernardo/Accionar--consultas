USE santander

SELECT 
 
 SANTIP.OCORRENCIA
,CASE WHEN(COUNT(SANDIS.CPC)) = 0 THEN 0
 ELSE (COUNT(SANDIS.CPC)) END AS QUANTIDADE_CPC 
,TIPO
--,MACRO
,DATA

FROM NOVO_SAN_DISCAGENS AS SANDIS
INNER JOIN SAN_TIPIFICACAO AS SANTIP ON SANDIS.TABULACAO COLLATE Latin1_General_CS_AI = SANTIP.OCORRENCIA COLLATE Latin1_General_CS_AI
WHERE SANDIS.ALO = 1 AND SANDIS.PERIODO = 202408 AND SANDIS.DATA = '2024-08-21'
AND SANDIS.CPC IS NOT NULL AND TIPO = 'IMPRODUTIVO'
GROUP BY OCORRENCIA, TIPO, DATA
ORDER BY OCORRENCIA
