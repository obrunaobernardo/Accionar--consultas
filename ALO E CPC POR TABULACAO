USE santander

SELECT 
 COUNT(SANDIS.CPC) AS QUANTIDADE_CPC
 --COUNT(SANDIS.ALO) AS QUANTIDADE_ALO
,SANDIS.TABULACAO
,SANDIS.PERIODO
,NOME_CARTEIRA

FROM NOVO_SAN_DISCAGENS AS SANDIS
INNER JOIN SAN_FOTO_CONTRATO_UNICO AS SANFCU ON SANDIS.COD_CAD  = SANFCU.COD_CAD 
AND NOME_CARTEIRA = 'LEVES'
WHERE ((SANDIS.DATA BETWEEN '2024-07-01' AND '2024-07-22') OR (SANDIS.DATA BETWEEN '2024-08-01' AND '2024-08-22')) AND SANDIS.CPC = 1 
GROUP BY SANDIS.PERIODO, NOME_CARTEIRA, SANDIS.TABULACAO
ORDER BY PERIODO, QUANTIDADE_CPC
