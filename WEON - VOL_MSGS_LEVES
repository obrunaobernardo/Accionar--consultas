SELECT

  --MAX(VOLUME_MENSAGENS) AS MAX_VOL_MSGS
 --,MIN(VOLUME_MENSAGENS) AS MIN_VOL_MSGS
 --,SUM(VOLUME_MENSAGENS) AS SUM_VOL_MSGS
VOLUME_MENSAGENS
,TEMPO_ATENDIMENTO

FROM [ipo].[dbo].[WEON_LEVES_TOTAL] AS WLT
INNER JOIN [santander].[dbo].[SAN_TIPIFICACAO] AS SANTIP ON WLT.QUALIFICACAO_FINAL = SANTIP.OCORRENCIA
WHERE CPC = 1 AND SUCESSO = 1 AND QUALIFICACAO_FINAL = 'ACORDO' AND VOLUME_MENSAGENS = 0
ORDER BY TEMPO_ATENDIMENTO
