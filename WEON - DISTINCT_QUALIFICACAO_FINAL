SELECT DISTINCT

 QUALIFICACAO_FINAL 
,CPC
,SUCESSO

FROM [ipo].[dbo].[WEON_LEVES_TOTAL] AS WLT
INNER JOIN [santander].[dbo].[SAN_TIPIFICACAO] AS SANTIP ON WLT.QUALIFICACAO_FINAL = SANTIP.OCORRENCIA
WHERE CPC = 1 --AND SUCESSO = 1
ORDER BY SUCESSO

/*
ORDER BY CPC DESC
OFFSET 300 ROWS FETCH NEXT 300 ROWS ONLY
*/ 




